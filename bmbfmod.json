{
  "id": "PracticePlus",
  "name": "PracticePlus",
  "version": "0.3.0",
  "author": "krx",
  "description": [
    "Updated version of the PracticePlus mod originally created by L_ : https://github.com/L025/PracticePlus",
    "This mod allows you to set the NJS and note offset for a song in practice mode, either from the practice menu or the pause menu."
  ],
  "category": "Gameplay",
  "gameVersion": "1.13.0",
  "platform": "Quest",
  "components": [
    {
      "type": "HookMod",
      "installAction": {
        "installLibraryFile": "libPracticePlus.so"
      },
      "uninstallAction": {
        "removeLibraryFile": "libPracticePlus.so"
      }
    },
    {
      "type": "HookModLibrary",
      "installAction": {
        "installLibraryFile": "libbeatsaber-hook_0_8_4.so"
      }
    },
    {
      "type": "HookModLibrary",
      "installAction": {
        "installLibraryFile": "libcustom-ui_0_1_4.so"
      }
    }
  ]
}
